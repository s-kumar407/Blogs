{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/usersProfilePics/(.*)",
      "dest": "/usersProfilePics/$1"
    },
    {
      "src": "/blogPics/(.*)",
      "dest": "/blogPics/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}

